{
  "name": "st-2-rest-api",
  "version": "1.0.0",
  "description": "Shorttrack NodeJS",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon -e ts  --exec",
    "build": "rimraf dist && tsc",
    "start": "npm run build && NODE_ENV=production node .",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts --fix",
    "pre-commit": "lint-staged",
    "typeorm": "NODE_ENV=production node -r ./node_modules/.bin/typeorm",
    "make:migration": "npm run typeorm -- migration:create -n",
    "migrate": "npx ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "npm run typeorm -- migration:revert",
    "db:seed": "npx ts-node-dev ./node_modules/typeorm-seeding/dist/cli.js seed",
    "test": "jest",
    "start-dev:migrate:seed": "npm run migrate && npm run db:seed && npm run dev",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "jest": {
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git@git.epam.com:aliaksandr_halaveika/st-2-rest-api.git"
  },
  "author": "Aliaksandr Halaveika",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.9.2",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.2.5",
    "eslint": "^7.32.0",
    "husky": "^7.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "2.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/http-errors": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
}
